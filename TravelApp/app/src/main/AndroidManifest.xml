<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ex.Travelapp">

    <application
        android:allowBackup="true"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="ex.Travelapp.DetailActivity"
            android:theme="@style/AppTheme1"/>
        <activity android:name="ex.Travelapp.Main2Activity"/>
        <activity
            android:name="ex.Travelapp.InfoActivity"
            android:theme="@style/AppTheme1" />
        <activity android:name="ex.Travelapp.RegisterActivity" />
        <activity android:name="ex.Travelapp.LoginActivity" />
        <activity
            android:name="ex.Travelapp.SearchActivity"
            android:theme="@style/AppTheme" />
        <activity
            android:name="ex.Travelapp.MainActivity"
            android:theme="@style/AppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>